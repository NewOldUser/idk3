-- PlaceId del juego (Steal a Brainrot)
local placeId = 109983668079237

-- Tabla para guardar los IDs de servidores visitados
local visitedServers = {}

-- Funci√≥n para hacer salto de servidor
local function hop()
    local HttpService = game:GetService("HttpService")
    local success, result = pcall(function()
        return HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?sortOrder=Desc&limit=100"))
    end)

    if success and result and result.data then
        for _, server in ipairs(result.data) do
            if server.playing < server.maxPlayers and not visitedServers[server.id] then
                visitedServers[server.id] = true
                wait(2) -- espera 2 segundos antes de saltar
                game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, server.id, game.Players.LocalPlayer)
                break
            end
        end
    end
end

-- Bucle para hacer saltos cada 2 segundos
while true do
    hop()
    wait(2)
end

